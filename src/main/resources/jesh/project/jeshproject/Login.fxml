<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="jesh.project.jeshproject.LoginPage">
    <TextField fx:id="usernameField" promptText="Username"/>
    <TextField fx:id="passwordField" promptText="Password"/>
    <Label fx:id="errorMessage" text = ""/>
    <Button fx:id="loginButton" text="Login" onAction="#login"/>
    <Button fx:id="goBackButton" onAction="#goBacktoHome" text="Go Back"/>
</VBox>
