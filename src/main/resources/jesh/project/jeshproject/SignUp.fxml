<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>

<?import javafx.scene.control.Label?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="jesh.project.jeshproject.controller.SignupPage">
    <Text fx:id="title">
        <font>
            <Font name="Arial Black" size="36.0" />
        </font>
    </Text>
    <TextField fx:id="firstNameField" promptText="First Name"/>
    <Label fx:id="firstNameErrorLabel"/>
    <TextField fx:id="lastNameField" promptText="Last Name"/>
    <Label fx:id="lastNameErrorLabel"/>
    <TextField fx:id="birthdayField" promptText="Birthday"/>
    <Label fx:id="birthdayErrorLabel"/>
    <TextField fx:id="emailField" promptText="Email"/>
    <Label fx:id="emailErrorLabel"/>
    <TextField fx:id="usernameField" promptText="Username"/>
    <Label fx:id="usernameErrorLabel"/>
    <TextField fx:id="passwordField" promptText="Password"/>
    <Label fx:id="passwordErrorLabel"/>
    <Button text="Sign Up" onAction="#signup"/>
    <Button fx:id="goBackButton" onAction="#goBacktoHome" text="Go Back"/>
</VBox>