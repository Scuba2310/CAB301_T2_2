<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.*?>


<VBox alignment="CENTER" spacing="40.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="jesh.project.jeshproject.controller.HomePage">
    <padding>
        <Insets bottom="60.0" left="20.0" right="20.0" top="01.0"/>
    </padding>
    <Text fx:id="title">
        <font>
            <Font name="Arial Black" size="36.0" />
        </font>
    </Text>
    <Button fx:id="loginButton" text="Login" onAction="#goToLoginPage"/>
    <Button fx:id="signupButton" text="Sign Up" onAction="#goToSignupPage"/>
</VBox>
